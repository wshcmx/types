{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": "tsconfig.json",
    "tsconfigRootDir": "tests"
  },
  "plugins": [
    "@typescript-eslint",
    "jsdoc"
  ],
  "rules": {
    "@typescript-eslint/no-wrapper-object-types": "off"
  }
}
